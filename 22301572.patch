diff --git a/scripts/run_scheduler_test.py b/scripts/run_scheduler_test.py
new file mode 100755
index 0000000..42f7aca
--- /dev/null
+++ b/scripts/run_scheduler_test.py
@@ -0,0 +1,41 @@
+#!/usr/bin/env python3
+import pexpect, sys, time
+
+cmd = 'make qemu'
+child = pexpect.spawn(cmd, encoding='utf-8', timeout=60)
+
+try:
+    child.expect_exact('$ ', timeout=60)
+except pexpect.TIMEOUT:
+    print('Timeout waiting for shell prompt', file=sys.stderr)
+    child.kill(0)
+    sys.exit(1)
+
+child.sendline('test_scheduler 10 &')
+child.sendline('test_scheduler 5 &')
+child.sendline('test_scheduler 2 &')
+
+time.sleep(2)
+
+child.send('\x10')
+time.sleep(1)
+child.send('\x10')
+time.sleep(1)
+
+out = ''
+try:
+    while True:
+        out += child.read_nonblocking(size=1024, timeout=0.5)
+except (pexpect.TIMEOUT, pexpect.EOF):
+    pass
+
+print('=== Captured output (tail): ===')
+print('\n'.join(out.splitlines()[-60:]))
+
+try:
+    child.send('\x03')
+except Exception:
+    pass
+child.expect(pexpect.EOF, timeout=10)
+
+sys.exit(0)
